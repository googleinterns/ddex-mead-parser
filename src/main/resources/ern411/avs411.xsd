            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supervising the technical aspects of a tracking Session.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TrackingSecondEngineer">
