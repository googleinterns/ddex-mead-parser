         <xs:enumeration value="TypeDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a typeface.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VideoDirector">
