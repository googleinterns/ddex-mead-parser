               <xs:documentation source="ddex:Definition">A Party responsible for sound production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating audio equipment to capture sound.</xs:documentation>
            </xs:annotation>
