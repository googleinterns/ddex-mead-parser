         <xs:enumeration value="SeriesProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party (individual or company) that is responsible for the production of a series.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SetDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer, also known as a stage designer, who is in charge of designing and creating the sets that appear in films, on television programmes as well as in the theatre.</xs:documentation>
