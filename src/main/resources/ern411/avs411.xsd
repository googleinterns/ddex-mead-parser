               <xs:documentation source="ddex:Definition">A MixingEngineer responsible for SurroundSound.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SurroundMixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MixingEngineer responsible for SurroundSound.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TapeOperator">
