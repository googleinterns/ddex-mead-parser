               <xs:documentation source="ddex:Definition">A Party creating special effects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Sponsor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party giving financial support for an Event or a Creation.</xs:documentation>
            </xs:annotation>
