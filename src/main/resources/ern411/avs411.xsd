               <xs:documentation source="ddex:Definition">A Director of a Video.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Videographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party in charge of a video production.</xs:documentation>
            </xs:annotation>
