         <xs:documentation source="ddex:Definition">A Type of CommercialModel (e.g. SubscriptionModel and PayAsYouGoModel). The CommercialModelType indicates how a Consumer pays for a Service or Release.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="AdvertisementSupportedModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel where the Service or Product offering is financed by revenue generated from the sale of advertising.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AsPerContract">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity used when a MessageSender wishes to indicate that the value within the allowed value set is defined by the contractual relationship between MessageSender and MessageRecipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DeviceFeeModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel in which revenues generated from the sale of devices are shared with rights holders. The relevant content does not need to be pre-loaded onto the device for the model to apply.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FreeOfChargeModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel in which a Resource, Release or Product is made available free of charge to Consumers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PayAsYouGoModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel where the Service or Product offering is financed by revenue generated from payment (set at any level but not zero) for each Usage which the Customer makes of the Service or Product.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PerformanceRoyaltiesModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel in which royalties are based on performances.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RightsClaimModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel in which a MessageSender is claiming ownership of rights in Release(s).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubscriptionModel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CommercialModel where the Service or Product offering is financed by revenue generated from a Customer Subscription.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Unknown">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An unknown CommercialModel. This allowed value is deprecated. DDEX advises that this value will be removed at a future date and therefore recommends against using it.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefined">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity which is defined by a sender of a DdexMessage in a manner acceptable to its recipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="CompositeMusicalWorkType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of CompositeMusicalWork.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Medley">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A MusicalWork whose expression is a combination of continuous and sequential existing MusicalWorks or excerpts from MusicalWorks. Note: A Medley does not normally result in the creation of a new copyright Work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Neither">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Neither a Medley nor a Potpourri.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Potpourri">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A MusicalWork whose expression is a combination of existing MusicalWorks or excerpts from MusicalWorks to which typically, additional original material has been added. Example: material may be added to provide musical links joining or combining the existing MusicalWorks. Note: A Potpourri typically results in the creation of a new piece of intellectual property and is sometimes known as Mashup.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ContainerFormat">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of container according to its FileFormat.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="AIFF">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Audio Interchange File Format</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AVI">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Audio Video Interleave.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MP4">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">MPEG-4 Part 14 or MP4 file format, formally ISO/IEC 14496-14:2003.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Ogg">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Multimedia container format maintained by the Xiph.Org Foundation</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="QuickTime">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">QuickTime as developed by Apple Inc.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RealMedia">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Multimedia container format created by RealNetworks.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RMF">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Rich Music Format, as defined by Beatnik Inc.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefined">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity which is defined by a sender of a DdexMessage in a manner acceptable to its recipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="WAV">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Waveform Audio File Format</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ContributorRole">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Role played by a Contributor.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Adapter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Author of adapted Lyrics of a MusicalWork. Note: The adapted Lyrics may or may not result in a new copyright Creation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Architect">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a building.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Arranger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A modifier of musical components of a Work. Note: The arranged MusicalWork may or may not result in a new copyright Creation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Author">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of written or spoken words which form part of a Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AuthorInQuotations">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party largely quoted in works, but which is not a direct Contributor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AuthorOfAfterword">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Author of an afterword.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Compiler">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party creating a work by selecting, arranging, aggregating, and editing other works.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Composer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of the musical elements of a MusicalWork. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ComposerLyricist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator that plays the roles of Composer and Lyricist of a MusicalWork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Conceptor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party on whose original idea a Work is based.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Creator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Maker of a Creation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DialogueAuthor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Author of dialogues.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dissertant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Person presenting a thesis for an academic degree.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Engraver">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a print from an engraved printing plate.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Etcher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a print from an etched printing plate.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Journalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of an article for a magazine or a newspaper.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LandscapeArchitect">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a land features and structures (e.g. in a park).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Librettist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a libretto.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Lithographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a print using lithography.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Lyricist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of the Lyrics of a MusicalWork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MetalEngraver">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of an engraved piece of metal (mainly for decorative purposes).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NonLyricAuthor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of written or spoken words other than Lyrics. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PlateMaker">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a print using a plate.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Playwright">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a stageplay.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Reporter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of an account of an Event.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Reviewer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is responsible for the review of a Creation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Rubricator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Facsimilist specializing in rubrication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScreenplayAuthor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a screenplay.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Sculptor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a sculpture.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubArranger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of arrangements made on behalf of a SubPublisher.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubLyricist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator who substitutes or modifies the existing Lyrics of a MusicalWork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Translator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that translates Lyrics and/or Text from one Language into another. This is also known as sub-Lyricist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Woodcutter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a print using woodcuts.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="WoodEngraver">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a print using wood engravings.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="WriterOfAccompanyingMaterial">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of textual material that accompanies audio or audiovisual material.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Publisher of books.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CopyrightClaimant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party listed as copyright owner at the time of registration.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CopyrightHolder">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party to whom copyright has been granted or transferred.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that enters into contracts and/or assignments with composer(s) in respect of their copyright MusicalWorks and in return for a proportion of the royalties generated from their use, undertakes work on behalf of the composer(s) including but not limited to promoting the usage of the Works, licensing their use, collecting and distributing royalties and developing writing partnerships with other composer(s). This term includes OriginalPublisher and SubPublisher. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NewspaperPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Publisher of newspapers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OriginalPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party which has acquired, from a Creator, Rights in a Creation for a specified Territory and Period.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PeriodicalPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Publisher of periodicals (e.g. magazines, journals).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party which has acquired, by Agreement with a MusicPublisher, Rights in one or more MusicalWorks for a specified Territory and Period. Note: This includes Rights which are passed to subsidiaries or affiliates of a larger Organization.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubstitutedPublisher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party acting on behalf of a MusicPublisher or other controller of Rights in a MusicalWork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Unknown">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity used when a sender of a DdexMessage wishes to indicate that the value within the allowed value set is unknown.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefined">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity which is defined by a sender of a DdexMessage in a manner acceptable to its recipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Accompanyist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who plays a musical Instrument while another Person is singing (or playing another instrument).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Actor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs spoken word or mime.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AdditionalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer that has worked on a smaller percentage of a project tham the main Engineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AdditionalMixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A SoundEngineer responsible for contributing additional mixes to a recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AdditionalPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer that is not a LeadPerformer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AdditionalProgrammingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An ProgrammingEngineer that has worked on a smaller percentage of a project than the main ProgrammingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AdditionalStudioProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A StudioProducer who makes a minor additional contribution to a recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnchorPerson">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party acting as the central moderator of a program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnimalTrainer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who trains and looks after animals.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Animator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates an animated work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Annotator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who makes annotations on a manuscript.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Announcer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party reading news or describing events.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AAndRAdministrator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party handling the administrative elements of the A&amp;R department and maintaining the budgets for artists signed to a label by working with accounting and legal personnel.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AAndRCoordinator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for searching new Artists for a record label (artist and repertoire coordinator).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Armourer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the supply and safety of weapons in a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArtCopyist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a copy of a piece of art.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArtDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director who oversees the Artists and craftspeople who build the sets.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Artist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A principal Contributor to a Performance of a MusicalWork or a NonMusicalWork that results in the creation of a Resource, or a principal creative Contributor to a Resource that is not a result of a Performance. Note: Used for naming groups as well as individuals.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArtistBackgroundVocalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for the recording of Artist Background Vocals.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArtistVocalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for the process of recording vocals on a project and dealing with technical issues and decisions. He or she operates the recording equipment (or supervises the operation of the recording equipment) during vocal Performances and makes aesthetic choices in order to obtain the sound and concepts the artist and producer are looking for. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArtistVocalSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an ArtistVocalEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantCameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a CameraOperator.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantChiefLightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the second head of the LightingTechnicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantConductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a Conductor. An AssistantConductor is not a subordinate to the Conductor, but rather a collaborator or at least on a somewhat equal level.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a Director.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an Editor (aka editorial assistant). This role is both an editorial and an administrative role.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for assisting another Engineer(s). They often keep all logs of Sessions and materials and operate equipment. They should also have enough engineering skills that they can step in for an Engineer in case of an absence.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is delegated some area of responsibility on a project, but reports to the Producer or CoProducer(s) (aka additional producer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantVisualEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assitant of a VisualEditor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssociatedPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist commonly associated with a Work as one of its Performers, and whose identity is only used for accurate Work identification.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssociateProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A junior Producer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AuralTrainer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who trains Performers in the development of aural skills.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BackgroundVocalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who sings in the background.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BalanceEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party primary or actively responsible for the process of recording and/or mixing a project as well as technical issues and decisions. The Engineer operates (or oversees the operation of) the equipment during the recording process and makes creative and aesthetic choices in collaboration with the Artist and producer in order to realize the sounds and concepts envisioned for the project. In the classical world, this is often referred to as a balance engineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BandLeader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The principal member of a MusicGroup.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Binder">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who binds an item.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BindingDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a book binding.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a book (including arrangement of type and illustration, choice of materials, etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookjackDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a dust jacket for a book.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookplateDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a bookplate (an identification label commonly on the inside front cover of books).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a book.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BroadcastAssistant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party providing general support in the production of a radio programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BroadcastJournalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party working as a Journalist for a broadcast programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Calligrapher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of artistic handwriting or lettering.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating a camera.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Carpenter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who carries out woodwork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Cartographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a map or other cartographic work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Cartoonist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a cartoon.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CastingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party selecting talent (Actors, singers, Dancers, etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Causeur">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who gives an informal lecture, typically on radio.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Censor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Editor who examines Creations and removes objectionable content.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChiefLightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the head of the LightingTechnicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Choir">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of Parties who perform vocally together. Typically, Choirs consist of at least 2 people in an combination of different vocal ranges.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChoirMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of a Choir.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Choreographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a dance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChorusMaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Conductor who directs Vocalists in a Choir.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CircusArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist with skills that are typical for circus performances.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClapperLoader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who operates the clapboard at the beginning of a shot.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClubDJ">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who announces and plays recorded music in a music club.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director who is sharing some or all of a director's responsabilities with a Director or another CoDirector.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoExecutiveProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An ExecutiveProducer who is not the only ExecutiveProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ColorSeparator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for separating the original artwork into individual color components for printing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Comedian">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who makes people laugh (by telling funny stories or by acting).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">One of a group of Artists who creates an original master album or single mix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoMixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is sharing some or all of a mixing engineer's responsabilities with a MixingEngineer or another CoMixer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Commentator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who provides interpretation, analysis, or a discussion of a subject matter.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CommissioningBroadcaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party commissioning material for a broadcast programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CompilationProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for supervising and/or exercising control over the work assigned to other Producers, Engineers, and others on the project.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ComputerGraphicCreator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of computer graphics.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ComputerProgrammer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a computer program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ConcertMaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The leader of the first violin section in an Orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Conductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who leads or conducts a Performance by a group of Musicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Consultant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party giving professional advice on matters of production technique or on content who does not appear in the programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ContinuityChecker">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who checks the continuity of a series of camera shots.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Contractor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is hired to perform work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer who is sharing some or all of a producer's responsabilities with a Producer or another CoProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Correspondent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the writer of a letter or other communication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CostumeDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a costume.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoverDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a cover.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dancer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a dance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Delineator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of technical drawings derived from designs made by other people.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Designer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a design.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DialogueCoach">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who helps train an Actor in diction and/or the use of dialects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DialogueDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for assembling, synchronizing, and editing all the dialogues in a film or television production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalAudioWorkstationEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer is responsible for the operation of a Digital Audio Workstation (DAW) during recording and mix sessions including the archiving and maintenance of the recorded material (basically data files). The DigitalAudioWorkstationEngineer is not generally called upon to make major aesthetic judgments.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for operating equipment for editing (in today’s world generally a digital audio workstation). They can, from time to time, be called on to make aesthetic choices.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a DigitalEditingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Director">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who leads or supervises actors, e.g. in the prodution of a movie.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DirectStreamDigitalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the functioning and maintenance of digital recordings using Direct Stream Digital format equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DistributionCompany">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Organization responsible for delivering material provided by a ProductionCompany.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DJ">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who announces and plays recorded music.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Draughtsman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a plan for constructing something.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who helps an Actor to put on and take off a costume.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dubber">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator who adds the dubbing into a Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Editor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the preparation of a publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditorInChief">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The head of editorial staff.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditorOfTheDay">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the preparation of a news broadcast of the day.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Encoder">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator processing and encoding a Resource for distribution.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Engineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for technical aspects. This includes RecordingEngineer, but may also cover roles in other content production processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Ensemble">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of two or more Parties performing a MusicalWork together. Note: An Ensemble may be of any size or any grouping of Performers from a vocal duo to a full orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ExecutiveProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who secures funding for a project. He or she may be responsible for the overall concept. He or she may monitor the project from time to time to ensure that the producer and Artist are on track.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Expert">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who contributes his expertise to a programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Facsimilist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a copy.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FightDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for the choreography of fight scenes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director of a movie.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmDistributor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Publisher of films.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who edits an audio-visual production for publishing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party (individual or company) that is responsible for the financing of an audio-visual production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmSoundEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for (part of) the soundtrack used in an audio-visual Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FloorManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party acting on the studio floor on behalf of the Director.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FocusPuller">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CameraOperator who adjusts the focus of the camera.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates incidental sound effects in post-production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who edits sound effects created by a FoleyArtist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A SoundMixer who works with a FoleyArtist to create sound effects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a drawing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicAssistant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party drawing pictures or picture elements as content for programmes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of graphical elements.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Greensman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party procuring and maintaining vegetation on a set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Grip">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supporting CameraOperators and LightingTechnicians with specialized equipment (e.g. camera cranes).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GuestConductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Conductor who is working with an Orchestra on a temporary basis.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GroupMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Person that is a member of a MusicGroup.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Hairdresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who prepares the hair of an Actor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Illustrator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of an illustration.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InitialProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that initiates the creation of a SoundRecording or Resource and is sometimes referred to as a commissioning rights holder. An InitialProducer may be a Person or an Organization and the term contrasts with the role of a StudioProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InterviewedGuest">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is interviewed.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Interviewer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who interviews, i.e. talks to somebody for the purpose of eliciting his opinions.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyCharacter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A central or primary Character in a storyline.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyGrip">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the head of the Grips.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyTalent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party performing as a leading character.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Leadman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is responsible for the crew in charge of the set decoration.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LeadPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer that plays the role of the protagonist in a film or play.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LeadVocalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is a lead Vocalist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LightingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the lighting.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating lighting equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LocationManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party managing all aspects relating to filming in specific places.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MakeUpArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who prepares the make up of an Actor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Manufacturer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for manufacturing a device or a piece of technical equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MasteringEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer who is the last creative bridge between the mix process and the distribution process. He or she generally works with a final mix of a project and makes final adjustments to the sound before distribution while maintaining the vision of the artist, producer and label.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MasteringSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MasteringEngineer (aka assistant mastering engineer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MatteArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates artworks used in making scenery and locations that do not exist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Mixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who creates an original master album or single mix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer combining different audio recording elements into a final version. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MixingEngineer (aka assistant mixing engineer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicArranger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Arranger who adds the final touches or subtle nuances to the musical arrangement of a recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicCopyist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a copy of a Composer or Arranger's manuscript.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for music in the production of a film or a radio program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicGroup">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of individuals who perform vocally and/or instrumentally together.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Musician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either vocally or instrumentally.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Narrator">
            <xs:annotation>
