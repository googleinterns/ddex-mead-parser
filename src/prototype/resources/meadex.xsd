      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="AwardingBody" type="mead:PartyDescriptor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the awarding body.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AwardedParty" type="mead:PartyWithNames">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the awarded Party.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AwardName" type="mead:NameWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name or Category of the award.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Date" type="mead:EventDate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the Date and Place of the award (in ISO 8601:2004 format: YYYY-MM-DD).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IsWinner" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the award was won (=true) or whether it was a nomination for the award (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Comment" minOccurs="0" maxOccurs="unbounded" type="mead:TextWithFormat">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Comment on the award.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice>
            <xs:element name="AwardedWork" type="mead:Work">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an awarded Work.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="AwardedResource" type="mead:Resource">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an awarded Resource.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="AwardedRelease" type="mead:Release">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an awarded Release.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BeatsPerMinute">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of beats.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="Value" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The value of the beats per minute.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Modulation" minOccurs="0" maxOccurs="unbounded" type="mead:Modulation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a modulation.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Biography">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a biography of a Party.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
