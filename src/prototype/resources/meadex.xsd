   <xs:element name="MeadMessage">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Message in the Media Enrichment And Description Standard.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="MessageHeader" type="mead:MessageHeader">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The MessageHeader for the MeadMessage.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="SubscriptionId" minOccurs="0" type="xs:string">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">An Identifier of the subscription.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="MetadataSourceList" minOccurs="0" type="mead:MetadataSourceList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Parties that created the metadata.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="PartyInformationList" minOccurs="0" type="mead:PartyInformationList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing detailed information about one or more Parties.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="WorkInformationList" minOccurs="0" type="mead:WorkInformationList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing detailed information about one or more Works</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ResourceInformationList" minOccurs="0"
               type="mead:ResourceInformationList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing detailed information about one or more Resources.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ReleaseInformationList" minOccurs="0"
               type="mead:ReleaseInformationList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing detailed information about one or more Releases.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:any namespace="##other" minOccurs="0" maxOccurs="unbounded"/>
         </xs:sequence>
         <xs:attribute name="LanguageAndScriptCode">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Language and script for the Elements of this message as defined in IETF RfC 5646. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern
                     value="[a-zA-Z]{2,3}(-[a-zA-Z]+){0,1}(-[a-zA-Z]{2}|-[0-9]{3}){0,1}(-[a-zA-Z][a-zA-Z0-9]{4}[a-zA-Z0-9]*){0,1}"
                  />
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="Feed">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Message in the Media Enrichment And Description Standard for an atom feed defined by W3C/IETF.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:choice minOccurs="3" maxOccurs="unbounded">
            <xs:element name="author" minOccurs="0" maxOccurs="unbounded" type="mead:Person">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of an author of a Resource.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="category" minOccurs="0" maxOccurs="unbounded" type="mead:Category">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a category.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="contributor" minOccurs="0" maxOccurs="unbounded" type="mead:Person">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of a contributor to a Resource.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="generator" minOccurs="0" type="mead:Generator">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a generator.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="icon" minOccurs="0" type="mead:Icon">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an icon.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="id" type="mead:Id">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an ID.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="link" minOccurs="0" maxOccurs="unbounded" type="mead:Link">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a link.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="logo" minOccurs="0" type="mead:Logo">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a logo.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="rights" minOccurs="0" type="mead:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of rights.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="subtitle" minOccurs="0" type="mead:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a subtitle.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="title" type="mead:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a title.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="updated" type="mead:DateTime">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an update.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="entry" minOccurs="0" maxOccurs="unbounded" type="mead:Entry">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an entry.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:any namespace="##other" minOccurs="0" maxOccurs="unbounded"/>
         </xs:choice>
         <xs:anyAttribute namespace="##other"/>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="AbsolutePitch">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an absolute pitch.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="Value" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The value of the absolute pitch of the A above middle C (a’) in Hertz.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AlternativeTitle">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an AlternativeTitle.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="TitleText" type="mead:TitleTextWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The text of the Title.</xs:documentation>
               <xs:documentation source="ddex:Comment">Further Reading: https://kb.ddex.net/display/HBK/How+long+can+artist+names+and+titles+be%3F</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SubTitle" minOccurs="0" type="mead:TitleTextWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A SubTitle of the Title, including Titles of Versions used to differentiate different versions of the same Title, as required by the GRid and ISRC ReferenceDescriptiveMetadataSets.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="LanguageAndScriptCode" type="xs:string">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The Language and script for the Elements of the Title as defined in IETF RfC 5646. The default is the same as indicated for the containing composite. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TitleType" type="avs:TitleType">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">A Type of the Title which defines its origin or the function it fulfils in relation to a Creation. Note: A Title may fulfil more than one role. Example: 'Help' may be both the OriginalTitle and the DisplayTitle for the well-known Beatles song. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Namespace" type="xs:string">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The Namespace of the TitleType. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="UserDefinedValue" type="xs:string">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">A UserDefined value of the TitleType. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="Annotation">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an Annotation, e.g. a Description or a Comment.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="Text" type="mead:TextWithFormat">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The text of the Annotation.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ArtistType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an artist type.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="Value" type="mead:ArtistTypeValue">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of an artist type value.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="ApplicableTerritoryCode" type="avs:CurrentTerritoryCode">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">A Territory to which the artist type applies. The use of ISO TerritoryCodes (or the term 'Worldwide') is strongly encouraged; TIS TerritoryCodes should only be used if both MessageSender and MessageRecipient are familiar with this standard. </xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="ArtistTypeValue">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an artist type value.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="avs:ArtistType">
            <xs:attribute name="Namespace" type="xs:string">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The Namespace of the element. This is represented in an XML schema as an XML Attribute.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="UserDefinedValue" type="xs:string">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A UserDefined value of the element. This is represented in an XML schema as an XML Attribute.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="ArtisticInfluence">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a related Artist that was influenced or that was an influencer.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="Party" minOccurs="0" maxOccurs="unbounded" type="mead:PartyWithNames">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Party.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Work" minOccurs="0" maxOccurs="unbounded" type="mead:Work">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Work.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Resource" minOccurs="0" maxOccurs="unbounded" type="mead:Resource">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Release" minOccurs="0" maxOccurs="unbounded" type="mead:Release">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Release.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Description" minOccurs="0" maxOccurs="unbounded"
            type="mead:TextWithFormat">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing Description of the relationship.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IsInfluenced" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the related Party is influenced by the current Party (=true) or not (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IsInfluencer" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the related Party has influenced the current Party (=true) or not (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Award">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an award.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="U[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="AwardingBody" type="mead:PartyDescriptor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the awarding body.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AwardedParty" type="mead:PartyWithNames">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the awarded Party.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AwardName" type="mead:NameWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name or Category of the award.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Date" type="mead:EventDate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the Date and Place of the award (in ISO 8601:2004 format: YYYY-MM-DD).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IsWinner" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the award was won (=true) or whether it was a nomination for the award (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Comment" minOccurs="0" maxOccurs="unbounded" type="mead:TextWithFormat">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Comment on the award.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AwardForParty">
