            </xs:simpleType>
         </xs:element>
         <xs:element name="Text" maxOccurs="unbounded" type="mead:BiographyText">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the biography text.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Author" minOccurs="0" maxOccurs="unbounded" type="mead:PartyWithNames">
